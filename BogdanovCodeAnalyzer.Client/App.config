<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.7.2" />
    </startup>
    <system.serviceModel>
        <bindings>
            <netNamedPipeBinding>
                <binding name="NetNamedPipeBinding_ITest">
                    <security mode="None" />
                </binding>
            </netNamedPipeBinding>
            <netTcpBinding>
                <binding name="ServiceBaseEndPoint">
                    <security>
                        <transport sslProtocols="None" />
                    </security>
                </binding>
            </netTcpBinding>
        </bindings>
        <client>
            <endpoint address="net.tcp://localhost:7255/ServiceBaseContact"
                binding="netTcpBinding" bindingConfiguration="ServiceBaseEndPoint"
                contract="ContractServiceReference.IServiceBaseContract" name="ServiceBaseEndPoint">
                <identity>
                    <userPrincipalName value="Богданов@eurotest.local" />
                </identity>
            </endpoint>
            <endpoint address="net.pipe://localhost/wcfsamples/test" binding="netNamedPipeBinding"
                bindingConfiguration="NetNamedPipeBinding_ITest" contract="ServiceReferenceCodeAnalyzer.ITest"
                name="NetNamedPipeBinding_ITest" />
        </client>
    </system.serviceModel>
</configuration>